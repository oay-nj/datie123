}
.picker-text::after {
  content: "";
  /* 必须设置为空 */
  position: absolute;
  right: 8px;
  /* 调整箭头位置 */
  top: 50%;
  transform: translateY(-50%);
  /* 垂直居中 */
  pointer-events: none;
  /* 确保点击事件不会触发箭头 */
  width: 0;
  height: 0;
  border-left: 4px solid transparent;
  border-right: 4px solid transparent;
  border-top: 8px solid black;
  text-align: center;
}
