@import "../../nvue.wxss";
.box-bg{flex:1;position:relative;width:750rpx;height:100vh;background-color:#fff}.content{flex:1;width:750rpx;height:100vh;background-color:#fff}.webview-box{flex:1;width:750rpx;height:calc(100vh - 44px - var(--status-bar-height))}.custom-navigation{position:fixed;left:0;right:0;height:44px;z-index:999;display:flex;flex-direction:row;align-items:center;background-color:#fff;padding:0 15px;box-shadow:0 1px 5px rgba(0,0,0,.1)}.nav-back{display:flex;align-items:center;padding:8px}.back-icon{width:20px;height:20px}.back-text{font-size:16px;color:#333;margin-left:4px}.nav-title{flex:1;text-align:center}.title-text{font-size:18px;color:#333;font-weight:500}
